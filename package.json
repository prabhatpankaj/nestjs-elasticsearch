  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env.development nest start",
    "start:dev": "env-cmd -f .env.development nest start --watch",
    "start:debug": "env-cmd -f .env.debug nest start --debug --watch",
    "start:development": "env-cmd -f .env.development node dist/main",
    "start:production": "env-cmd -f .env.production.js node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f .env.test jest",
    "test:watch": "env-cmd -f .env.test jest --watch",
    "test:cov": "env-cmd -f .env.test jest --coverage",
    "test:debug": "env-cmd -f .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f .env.test jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
